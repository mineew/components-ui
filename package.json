{
  "name": "@mineew/components-ui",
  "version": "0.1.2",
  "description": "React UI Components Library",
  "author": "Alexandr Mineev <mineew.al@gmail.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.(t|j)s?(x)\"",
    "format:check": "prettier --check \"**/*.(t|j)s?(x)\"",
    "clear": "rimraf lib",
    "clear:cache": "rimraf node_modules/.cache",
    "deps:check": "ncu",
    "deps:update": "ncu -ui",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@heroicons/react": "2.0.17",
    "@zag-js/checkbox": "0.7.0",
    "@zag-js/react": "0.7.0",
    "@zag-js/select": "0.7.0",
    "@zag-js/tooltip": "0.7.0",
    "classnames": "2.3.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "autoprefixer": "10.4.14",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "npm-check-updates": "16.10.10",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  }
}
